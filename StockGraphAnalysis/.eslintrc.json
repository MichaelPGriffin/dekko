{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc":"off",
        "linebreak-style":"off",
        "arrow-parens":"off",
        "object-curly-spacing":"off",
        "eqeqeq":"error",
        "no-undef":"error",
        "comma-dangle":["error", "never"]
    },
    "overrides": [
        {
          "files": ["test/**"],
          "rules": {
              "no-undef":"off"
           }
        }
    ]
}
